.row
   .col-xs-12(ng-include="'private/userSpaceMenu.html'")
.row
   .col-xs-3(ng-include="'private/userBar/userBar.html'")
   .col-xs-9
      .row.panel
         .input-group.exercises__search
            input.form-control(type='text', placeholder='Search for...')
            span.input-group-btn
               button.btn.btn-default(type='button') Go!
      .row
         .col-sm-6.col-md-4(ng-repeat="exercise in exercises")
            .thumbnail
               //img(data-src='holder.js/100%x200', alt='Generic placeholder thumbnail')
               .caption
                  h3(ng-bind="exercise.name")
                  p(ng-bind="exercise.description")
                  p
                     a.btn.btn-primary(href='#', role='button') Button
                     a.btn.btn-default(href='#', role='button') Button